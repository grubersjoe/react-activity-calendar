{
  "name": "react-activity-calendar",
  "version": "2.7.1",
  "description": "React component to display activity data in calendar",
  "author": "Jonathan Gruber <gruberjonathan@gmail.com>",
  "license": "MIT",
  "homepage": "https://grubersjoe.github.io/react-activity-calendar/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/grubersjoe/react-activity-calendar.git"
  },
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "files": ["build"],
  "scripts": {
    "build": "rollup -c",
    "build:storybook": "storybook build -o docs/ && touch docs/.nojekyll",
    "bundlesize": "pnpm install && EXTERNAL_DEPS=false pnpm build && open bundle.html",
    "check": "tsc && pnpm lint && pnpm test",
    "dev": "storybook dev -p 9000",
    "format": "prettier --write .",
    "lint": "eslint .",
    "postbuild": "dts-bundle-generator src/index.tsx -o build/index.d.ts --no-check --no-banner --external-imports react date-fns",
    "prepublishOnly": "tsc && pnpm lint && pnpm build",
    "start": "rollup -c -w",
    "test": "jest"
  },
  "dependencies": {
    "date-fns": "^4.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.7",
    "@babel/preset-env": "^7.25.7",
    "@babel/preset-react": "^7.25.7",
    "@babel/preset-typescript": "^7.25.7",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.5",
    "@eslint/compat": "^1.2.1",
    "@eslint/js": "^9.13.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.3.1",
    "@jest/globals": "^29.7.0",
    "@jest/types": "^29.6.3",
    "@mui/material": "^6.1.4",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^28.0.1",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@storybook/addon-docs": "^8.3.6",
    "@storybook/addon-essentials": "^8.3.6",
    "@storybook/addon-links": "^8.3.6",
    "@storybook/blocks": "^8.3.6",
    "@storybook/builder-vite": "^8.3.6",
    "@storybook/react": "^8.3.6",
    "@storybook/react-vite": "^8.3.6",
    "@storybook/theming": "^8.3.6",
    "@types/eslint__js": "^8.42.3",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.0",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@vitejs/plugin-react": "^4.3.3",
    "dts-bundle-generator": "^9.5.1",
    "eslint": "^9.13.0",
    "eslint-plugin-react": "^7.37.1",
    "eslint-plugin-react-hooks": "^5.0.0",
    "globals": "^15.10.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.3.2",
    "prism-react-renderer": "^2.3.1",
    "react": "^18.3.1",
    "react-docgen": "^7.1.0",
    "react-dom": "^18.3.1",
    "react-syntax-highlighter": "^15.6.1",
    "react-tooltip": "^5.27.0",
    "rollup": "^4.24.0",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-filesize": "^10.0.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-visualizer": "^5.12.0",
    "storybook": "^8.3.6",
    "storybook-dark-mode": "^4.0.2",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.11.0",
    "vite": "^5.4.8",
    "vite-plugin-replace": "^0.1.1"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "jest": {
    "testEnvironment": "jsdom"
  },
  "browserslist": ["last 1 chrome version", "last 1 firefox version", "last 1 safari version"]
}
